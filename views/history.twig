<h4>Submission History</h4>
<table class="highlight ">
    <thead>
        <tr>
            <th>Date</th>
            <th>Origin</th>
            <th>User IP</th>
            <th>Params</th>
        </tr>
    </thead>

    <tbody>
        {% for entry in history %}
            <tr>
                <td>{{ entry.created_at }}</td>
                <td>{{ entry.origin }}</td>
                <td>{{ entry.user_ip }}</td>
                <td>{{ entry.template_params|json_encode(constant('JSON_PRETTY_PRINT')) }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

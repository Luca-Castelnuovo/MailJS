<div id="template-{{ template.id }}-history" class="modal">
    <div class="modal-content">
        <h4>Submission History</h4>
        <table class="highlight ">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Origin</th>
                    <th>User IP</th>
                    <th>Params</th>
                </tr>
            </thead>

            <tbody>
                {% for history in template.history %}
                    <tr>
                        <td>{{ history.created_at }}</td>
                        <td>{{ history.origin }}</td>
                        <td>{{ history.user_ip }}</td>
                        <td>{{ history.template_params|json_encode(constant('JSON_PRETTY_PRINT')) }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>
    <div class="modal-footer">
        <button class="btn blue accent-4 waves-effect waves-light modal-close">Close</button>
    </div>
</div>
